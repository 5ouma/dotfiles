[core]
	editor = code --wait
[init]
	defaultBranch = main
[merge]
	tool = code --wait \"$MERGED\"
[push]
	default = simple
[url "github:5ouma"]
	InsteadOf = https://github.com/5ouma
	InsteadOf = git@github.com:5ouma
[url "github-school:Souma-S"]
	InsteadOf = https://github.com/Souma-S
	InsteadOf = git@github.com:Souma-S
[url "github-school:Course-Programming-II"]
	InsteadOf = https://github.com/Course-Programming-II
	InsteadOf = git@github.com:Course-Programming-II
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
